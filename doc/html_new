<script language="javascript" type="text/javascript">
window.allow_editor_invite = 2;
window.allow_editor_task = 1;
window.resp_selected = false;
window.resp_forced = false;
window.resp_def = 101419;
function resp_force(id) {
  if (id) window.resp_forced = true;
  if(!window.resp_selected && window.resp_forced) $('#rsp' + (id ? id : resp_def)).click();
}
function resp_select(el, id, id_group) {
  if ($(last_dropmenu).attr('id') == 'dropresp') window.resp_selected = true;
  $('#dropresp').parent().addClass('user_to_act');
  $('#dropresp').find('b').html($(el).html());
  $('#dropresp').next().val(id);
  if ($(last_dropmenu).attr('id') == 'dropresp') drophide();
  member_to(id); }
function resp_delete(el) {
  if ($(el).parent().hasClass('user_to_act')) {
    $(el).parent().removeClass('user_to_act'); 
    $(el).next().next().next().next().val(1).prev().find('b').html('<span class=\"av_sm av_def av_def0\" ></span>Без ответственного');
    member_to(0);   } 
}
</script>
